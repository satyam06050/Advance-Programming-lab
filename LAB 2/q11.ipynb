   "source": [
    "# Q.11) Convert alphabet to opposite case"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "char = input(\"Enter an alphabet: \")\n",
    "\n",
    "# Convert to opposite case\n",
    "if char.isupper():\n",
    "    result = char.lower()\n",
    "else:\n",
    "    result = char.upper()\n",
    "\n",
    "print(f\"Original: {char}\")\n",
    "print(f\"Opposite case: {result}\")"
   ]
  }
