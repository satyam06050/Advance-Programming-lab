    "# Q.13) Find distinct prime factors of a number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "num = int(input(\"Enter a number: \"))\n",
    "original_num = num\n",
    "factors = []\n",
    "\n",
    "for i in range(2, num):\n",
    "    if num % i == 0:\n",
    "        factors.append(i)\n",
    "        \n",
    "print(f\"Input: {original_num}\")\n",
    "print(f\"Output: {factors}\")"
   ]
  }
