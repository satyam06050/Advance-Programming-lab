   "source": [
    "words = input(\"Enter words: \").split()\n",
    "freq = {}\n",
    "for w in words:\n",
    "    if w in freq:\n",
    "        freq[w] += 1\n",
    "    else:\n",
    "        freq[w] = 1\n",
    "print(freq)"
