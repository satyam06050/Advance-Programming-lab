    "# Q.9) Sum of list elements using recursive function"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def sum_list(lst):\n",
    "    if len(lst) == 0:\n",
    "        return 0\n",
    "    return lst[0] + sum_list(lst[1:])\n",
    "\n",
    "nums = list(map(int, input(\"Enter numbers: \").split()))\n",
    "print(\"Sum:\", sum_list(nums))"
   ]
  }
