    "# Q.10) Calculate square and cube using user-defined function"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def calc(nums):\n",
    "    for n in nums:\n",
    "        print(f\"{n}: square={n*n}, cube={n*n*n}\")\n",
    "\n",
    "numbers = list(map(int, input(\"Enter numbers: \").split()))\n",
    "calc(numbers)"
   ]
  }
