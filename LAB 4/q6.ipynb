    "# Q.6) Sort dictionary by key and remove specific key"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "d = {\"c\": 3, \"a\": 1, \"b\": 2}\n",
    "print(\"Original:\", d)\n",
    "\n",
    "sorted_d = dict(sorted(d.items()))\n",
    "print(\"Sorted:\", sorted_d)\n",
    "\n",
    "key = input(\"Key to remove: \")\n",
    "if key in sorted_d:\n",
    "    del sorted_d[key]\n",
    "print(\"After removal:\", sorted_d)"
   ]
  }
