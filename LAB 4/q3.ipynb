   "source": [
    "# Q.3) Common elements raised to power of 2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "d18765dd",
   "metadata": {},
   "outputs": [],
   "source": [
    "set1 = {1, 2, 3, 4, 5}\n",
    "set2 = {3, 4, 5, 6, 7}\n",
    "\n",
    "common = set1.intersection(set2)\n",
    "result = {x**2 for x in common}\n",
    "\n",
    "print(f\"Set1: {set1}\")\n",
    "print(f\"Set2: {set2}\")\n",
    "print(f\"Common elements: {common}\")\n",
    "print(f\"Common elements squared: {result}\")"
   ]
  }
