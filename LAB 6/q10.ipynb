    "# Q.10) Find common elements in two arrays without loops"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "b70edbae",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "\n",
    "A = np.array([1, 2, 3, 4, 5, 6])\n",
    "B = np.array([4, 5, 6, 7, 8, 9])\n",
    "\n",
    "common = np.intersect1d(A, B)\n",
    "\n",
    "print(f\"Array A: {A}\")\n",
    "print(f\"Array B: {B}\")\n",
    "print(f\"Common elements: {common}\")"
   ]
  }
