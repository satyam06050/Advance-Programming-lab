   "source": [
    "import numpy as np\n",
    "\n",
    "arr_3d = np.arange(27).reshape(3, 3, 3)\n",
    "print(f\"3D Array (3x3x3):\\n{arr_3d}\")\n",
    "\n",
    "middle_rows = arr_3d[:, 1, :]\n",
    "print(f\"\\nMiddle row of each 2D subarray (3x3):\\n{middle_rows}\")"
   ]
