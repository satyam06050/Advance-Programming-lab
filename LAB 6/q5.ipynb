   "source": [
    "# Q.5) Create 10x10 array with 1s on perimeter and 0s inside"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "f58fbbb7",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "\n",
    "arr = np.zeros((10, 10), dtype=int)\n",
    "arr[0, :] = 1\n",
    "arr[-1, :] = 1\n",
    "arr[:, 0] = 1\n",
    "arr[:, -1] = 1\n",
    "\n",
    "print(arr)"
   ]
  }
