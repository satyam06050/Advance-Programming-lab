   "source": [
    "# Q.8) Find frequency and index positions of element in tuple"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "2543cc10",
   "metadata": {},
   "outputs": [],
   "source": [
    "tup = (1, 2, 3, 4, 3, 5, 3, 6, 3)\n",
    "element = 3\n",
    "\n",
    "frequency = tup.count(element)\n",
    "print(f\"Frequency of {element}: {frequency}\")\n",
    "\n",
    "indices = [i for i, x in enumerate(tup) if x == element]\n",
    "print(f\"Index positions: {indices}\")"
   ]
  }
