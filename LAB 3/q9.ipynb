   "source": [
    "# Q.9) Insert element at specific position in tuple, make mutable/immutable"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "3b9b8286",
   "metadata": {},
   "outputs": [],
   "source": [
    "tup = (1, 2, 3, 4, 5)\n",
    "print(f\"Original tuple: {tup}\")\n",
    "\n",
    "lst = list(tup)\n",
    "lst.insert(2, 99)\n",
    "tup = tuple(lst)\n",
    "\n",
    "print(f\"After inserting 99 at position 2: {tup}\")\n",
    "print(f\"Type: {type(tup)}\")"
   ]
  }
