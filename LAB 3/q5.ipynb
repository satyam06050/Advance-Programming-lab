   "source": [
    "# Q.5) Remove specific element and clear list"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "5d6e6118",
   "metadata": {},
   "outputs": [],
   "source": [
    "lst = [1, 2, 3, 4, 5, 3, 6, 7, 3]\n",
    "element = 3\n",
    "\n",
    "while element in lst:\n",
    "    lst.remove(element)\n",
    "\n",
    "print(f\"After removing {element}: {lst}\")\n",
    "\n",
    "lst.clear()\n",
    "print(f\"After clearing list: {lst}\")"
   ]
  }
